{
  "name": "@gunnarx2/root",
  "private": false,
  "author": "gunnarx2",
  "license": "MIT",
  "homepage": "https://github.com/gunnarx2/gunnarx2-mono#readme",
  "scripts": {
    "root:setup": "npm-run-all lerna:clean root:install lerna:bootstrap lerna:run:typescript lerna:run:build",
    "root:create": "ts-node scripts/create",
    "root:install": "yarn",
    "root:install:ci": "yarn --frozen-lockfile --ignore-engines",
    "lerna:publish": "lerna publish from-package --yes",
    "lerna:add": "lerna add",
    "lerna:clean": "rm -rf node_modules && lerna clean --yes && lerna exec -- rm -rf ./lib",
    "lerna:version:pre": "npm-run-all root:install:ci lerna:bootstrap:ci lerna:run:typescript lerna:run:build eslint:report lerna:run:test:ci",
    "lerna:version:patch": "yarn lerna:version:pre && lerna version patch",
    "lerna:version:minor": "yarn lerna:version:pre && lerna version minor",
    "lerna:version:major": "yarn lerna:version:pre && lerna version major",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:bootstrap:ci": "lerna bootstrap -- --frozen-lockfile --ignore-engines",
    "lerna:run:test:ci": "lerna run test:ci",
    "lerna:run:test:watch": "lerna run test:watch --parallel",
    "lerna:run:typescript": "lerna run typescript",
    "lerna:run:build": "lerna run build",
    "prettier:report": "prettier '**/*.{js,tsx,ts}' --check",
    "prettier:fix": "prettier '**/*.{js,tsx,ts}' --write",
    "eslint:report": "eslint '**/*.{js,tsx,ts}'",
    "eslint:fix": "eslint '**/*.{js,tsx,ts}' --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-modules-umd": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@gunnarx2/eslint-config-eslint-react": "^4.0.2",
    "@gunnarx2/prettier": "^4.0.0",
    "@octokit/core": "^3.2.0",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "inquirer": "^7.3.3",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunnarx2/gunnarx2-mono.git"
  },
  "bugs": {
    "url": "https://github.com/gunnarx2/gunnarx2-mono/issues"
  }
}
